def and(x, y):
  return x*y

def or(x, y):
  return if (x + y) == 0 then 0 else 1 fi

def xor(x, y):
  return if (x + y) == 1 then 1 else 0 fi

def not(x):
  return if x == 0 then 1 else 0 fi

// returns 1 if arg is a valid bit (0 or 1)
def checkBit(x):
  return if (x)*(x - 1) == 0 then 1 else 0 fi

